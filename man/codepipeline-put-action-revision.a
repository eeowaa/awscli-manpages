\-\-pipeline\-name <value>
\-\-stage\-name <value>
\-\-action\-name <value>
\-\-action\-revision <value>
[\-\-cli\-input\-json | \-\-cli\-input\-yaml]
[\-\-generate\-cli\-skeleton <value>]
.ft P
.fi
.UNINDENT
.UNINDENT
