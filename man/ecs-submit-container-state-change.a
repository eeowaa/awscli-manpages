[\-\-cluster <value>]
[\-\-task <value>]
[\-\-container\-name <value>]
[\-\-runtime\-id <value>]
[\-\-status <value>]
[\-\-exit\-code <value>]
[\-\-reason <value>]
[\-\-network\-bindings <value>]
[\-\-cli\-input\-json | \-\-cli\-input\-yaml]
[\-\-generate\-cli\-skeleton <value>]
.ft P
.fi
.UNINDENT
.UNINDENT
