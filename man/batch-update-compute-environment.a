{
    "computeEnvironmentName": "P2OnDemand",
    "computeEnvironmentArn": "arn:aws:batch:us\-east\-1:012345678910:compute\-environment/P2OnDemand"
}
.ft P
.fi
.UNINDENT
.UNINDENT
