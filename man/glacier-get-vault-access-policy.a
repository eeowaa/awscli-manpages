        "Policy": "{\e"Version\e":\e"2012\-10\-17\e",\e"Statement\e":[{\e"Effect\e":\e"Allow\e",\e"Principal\e":{\e"AWS\e":\e"arn:aws:iam::444455556666:root\e"},\e"Action\e":\e"glacier:ListJobs\e",\e"Resource\e":\e"arn:aws:glacier:us\-east\-1:111122223333:vaults/example_vault\e"},{\e"Effect\e":\e"Allow\e",\e"Principal\e":{\e"AWS\e":\e"arn:aws:iam::444455556666:root\e"},\e"Action\e":\e"glacier:UploadArchive\e",\e"Resource\e":\e"arn:aws:glacier:us\-east\-1:111122223333:vaults/example_vault\e"}]}"
