[\-\-presigned\-url <value>]
\-\-source\-region <value>
\-\-source\-snapshot\-id <value>
[\-\-tag\-specifications <value>]
[\-\-dry\-run | \-\-no\-dry\-run]
[\-\-cli\-input\-json | \-\-cli\-input\-yaml]
[\-\-generate\-cli\-skeleton <value>]
.ft P
.fi
