            }
        ],
        "GlobalTableArn": "arn:aws:dynamodb::123456789012:global\-table/MusicCollection",
        "CreationDateTime": 1576625818.532,
        "GlobalTableStatus": "CREATING",
        "GlobalTableName": "MusicCollection"
    }
}
.ft P
.fi
.UNINDENT
