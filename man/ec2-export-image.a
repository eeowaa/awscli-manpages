.INDENT 0.0
.INDENT 3.5
.sp
.nf
.ft C
{
    "DiskImageFormat": "vmdk",
    "ExportImageTaskId": "export\-ami\-1234567890abcdef0"
    "ImageId": "ami\-1234567890abcdef0",
    "RoleName": "vmimport",
    "Progress": "0",
    "S3ExportLocation": {
        "S3Bucket": "my\-export\-bucket",
        "S3Prefix": "exports/"
    },
    "Status": "active",
    "StatusMessage": "validating"
}
.ft P
.fi
.UNINDENT
