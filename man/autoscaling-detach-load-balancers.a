\-\-auto\-scaling\-group\-name <value>
\-\-load\-balancer\-names <value>
[\-\-cli\-input\-json | \-\-cli\-input\-yaml]
[\-\-generate\-cli\-skeleton <value>]
.ft P
.fi
.UNINDENT
.UNINDENT
