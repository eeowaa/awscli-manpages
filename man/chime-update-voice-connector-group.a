{
    "VoiceConnectorGroup": {
        "VoiceConnectorGroupId": "123a456b\-c7d8\-90e1\-fg23\-4h567jkl8901",
        "Name": "newGroupName",
        "VoiceConnectorItems": [
            {
                "VoiceConnectorId": "abcdef1ghij2klmno3pqr4",
                "Priority": 1
            }
        ],
        "CreatedTimestamp": "2019\-09\-18T16:38:34.734Z",
        "UpdatedTimestamp": "2019\-10\-28T19:00:57.081Z"
    }
}
