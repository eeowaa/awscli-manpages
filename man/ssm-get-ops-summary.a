[\-\-sync\-name <value>]
[\-\-filters <value>]
[\-\-aggregators <value>]
[\-\-result\-attributes <value>]
[\-\-cli\-input\-json | \-\-cli\-input\-yaml]
[\-\-starting\-token <value>]
[\-\-page\-size <value>]
[\-\-max\-items <value>]
[\-\-generate\-cli\-skeleton <value>]
.ft P
.fi
