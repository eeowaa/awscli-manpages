.INDENT 0.0
.INDENT 3.5
.sp
.nf
.ft C
aws connect update\-user\-hierarchy \e
    \-\-hierarchy\-group\-id 12345678\-a1b2\-c3d4\-e5f6\-123456789abc \e
    \-\-user\-id 87654321\-2222\-1234\-1234\-111234567891 \e
    \-\-instance\-id a1b2c3d4\-5678\-90ab\-cdef\-EXAMPLE11111
.ft P
.fi
.UNINDENT
.UNINDENT
