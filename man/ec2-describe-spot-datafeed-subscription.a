.INDENT 0.0
.INDENT 3.5
.sp
.nf
.ft C
  describe\-spot\-datafeed\-subscription
[\-\-dry\-run | \-\-no\-dry\-run]
[\-\-cli\-input\-json | \-\-cli\-input\-yaml]
[\-\-generate\-cli\-skeleton <value>]
.ft P
.fi
.UNINDENT
.UNINDENT
