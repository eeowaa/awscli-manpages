{
    "requestId": "a1b2c3d4\-5678\-90ab\-cdef\-EXAMPLE11111",
    "imagePipelineArn": "arn:aws:imagebuilder:us\-west\-2:123456789012:image\-pipeline/mywindows2016pipeline"
