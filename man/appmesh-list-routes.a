        {
            "arn": "arn:aws:appmesh:us\-east\-1:123456789012:mesh/app1/virtualRouter/vrServiceB/route/toVnServiceB",
            "meshName": "app1",
            "routeName": "toVnServiceB\-weighted",
            "virtualRouterName": "vrServiceB"
        }
    ]
}
.ft P
.fi
.UNINDENT
.UNINDENT
.sp
