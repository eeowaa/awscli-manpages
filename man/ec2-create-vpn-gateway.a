.INDENT 3.5
.sp
.nf
.ft C
  create\-vpn\-gateway
[\-\-availability\-zone <value>]
\-\-type <value>
[\-\-tag\-specifications <value>]
[\-\-amazon\-side\-asn <value>]
[\-\-dry\-run | \-\-no\-dry\-run]
[\-\-cli\-input\-json | \-\-cli\-input\-yaml]
[\-\-generate\-cli\-skeleton <value>]
.ft P
.fi
