    "SpotInstanceType": "one\-time"|"persistent",
    "BlockDurationMinutes": integer,
    "ValidUntil": timestamp,
    "InstanceInterruptionBehavior": "hibernate"|"stop"|"terminate"
  }
}
.ft P
.fi
