.ft C
  create\-connection
[\-\-provider\-type <value>]
\-\-connection\-name <value>
[\-\-tags <value>]
[\-\-host\-arn <value>]
[\-\-cli\-input\-json | \-\-cli\-input\-yaml]
[\-\-generate\-cli\-skeleton <value>]
.ft P
.fi
.UNINDENT
.UNINDENT
