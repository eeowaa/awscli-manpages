\-\-resource\-specification <value>
\-\-file\-format <value>
[\-\-file\-password <value>]
[\-\-cli\-input\-json | \-\-cli\-input\-yaml]
[\-\-generate\-cli\-skeleton <value>]
.ft P
.fi
.UNINDENT
.UNINDENT
