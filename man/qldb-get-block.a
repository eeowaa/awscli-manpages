.INDENT 3.5
.sp
.nf
.ft C
{
    "IonText": "{strandId:\e"KmA3ZZca7vAIiJAK9S5Iwl\e",sequenceNo:100}"
}
.ft P
.fi
.UNINDENT
.UNINDENT
.sp
