.ft C
{
    "domainConfigurationName": "additionalDataDomain",
    "domainConfigurationArn": "arn:aws:iot:us\-west\-2:123456789012:domainconfiguration/additionalDataDomain/dikMh"
}
.ft P
.fi
.UNINDENT
.UNINDENT
.sp
