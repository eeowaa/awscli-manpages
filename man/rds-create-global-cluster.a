[\-\-database\-name <value>]
[\-\-storage\-encrypted | \-\-no\-storage\-encrypted]
[\-\-cli\-input\-json | \-\-cli\-input\-yaml]
[\-\-generate\-cli\-skeleton <value>]
.ft P
.fi
.UNINDENT
