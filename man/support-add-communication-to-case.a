[\-\-case\-id <value>]
\-\-communication\-body <value>
[\-\-cc\-email\-addresses <value>]
[\-\-attachment\-set\-id <value>]
[\-\-cli\-input\-json | \-\-cli\-input\-yaml]
[\-\-generate\-cli\-skeleton <value>]
.ft P
.fi
