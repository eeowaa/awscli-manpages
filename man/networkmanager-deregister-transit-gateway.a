        "GlobalNetworkId": "global\-network\-01231231231231231",
        "TransitGatewayArn": "arn:aws:ec2:us\-west\-2:123456789012:transit\-gateway/tgw\-123abc05e04123abc",
        "State": {
            "Code": "DELETING"
        }
    }
}
.ft P
.fi
