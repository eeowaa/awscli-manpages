.ft C
  create\-virtual\-service
[\-\-client\-token <value>]
\-\-mesh\-name <value>
[\-\-mesh\-owner <value>]
\-\-spec <value>
[\-\-tags <value>]
\-\-virtual\-service\-name <value>
[\-\-cli\-input\-json | \-\-cli\-input\-yaml]
[\-\-generate\-cli\-skeleton <value>]
.ft P
.fi
