    \-\-resource\-arn arn:aws:networkmanager::123456789012:device/global\-network\-01231231231231231/device\-07f6fd08867abc123 \e
    \-\-tags Key=Network,Value=Northeast \e
    \-\-region us\-west\-2
.ft P
.fi
