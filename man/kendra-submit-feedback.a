.ft C
  submit\-feedback
\-\-index\-id <value>
\-\-query\-id <value>
[\-\-click\-feedback\-items <value>]
[\-\-relevance\-feedback\-items <value>]
[\-\-cli\-input\-json | \-\-cli\-input\-yaml]
[\-\-generate\-cli\-skeleton <value>]
.ft P
.fi
.UNINDENT
