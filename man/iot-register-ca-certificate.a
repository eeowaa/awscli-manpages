{
    "certificateArn": "arn:aws:iot:us\-west\-2:123456789012:cacert/f4efed62c0142f16af278166f61962501165c4f0536295207426460058cd1467",
    "certificateId": "f4efed62c0142f16af278166f61962501165c4f0536295207426460058cd1467"
 }
.ft P
.fi
.UNINDENT
.UNINDENT
.sp
