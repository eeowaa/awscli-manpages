.INDENT 3.5
.sp
.nf
.ft C
  describe\-groups
[\-\-authentication\-token <value>]
\-\-search\-query <value>
[\-\-organization\-id <value>]
[\-\-cli\-input\-json | \-\-cli\-input\-yaml]
[\-\-starting\-token <value>]
[\-\-page\-size <value>]
[\-\-max\-items <value>]
[\-\-generate\-cli\-skeleton <value>]
.ft P
.fi
.UNINDENT
.UNINDENT
