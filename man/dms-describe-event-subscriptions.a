{
    "EventSubscriptionsList": [
        {
            "CustomerAwsId": "123456789012",
            "CustSubscriptionId": "my\-dms\-events",
            "SnsTopicArn": "arn:aws:sns:us\-east\-1:123456789012:my\-sns\-topic",
            "Status": "deleting",
            "SubscriptionCreationTime": "2020\-05\-21 22:28:51.924",
            "Enabled": true
        }
    ]
}
.ft P
.fi
