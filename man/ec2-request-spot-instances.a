  "ImageId": "ami\-1a2b3c4d",
  "KeyName": "my\-key\-pair",
  "SecurityGroupIds": [ "sg\-1a2b3c4d" ],
  "InstanceType": "m3.medium",
  "Placement": {
    "AvailabilityZone": "us\-west\-2a"
  },
  "IamInstanceProfile": {
      "Arn": "arn:aws:iam::123456789012:instance\-profile/my\-iam\-role"
  }
}
.ft P
.fi
