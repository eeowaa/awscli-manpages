[\-\-next\-token <value>]
[\-\-max\-results <value>]
\-\-domain\-name <value>
\-\-key\-name <value>
\-\-values <value>
[\-\-cli\-input\-json | \-\-cli\-input\-yaml]
[\-\-generate\-cli\-skeleton <value>]
.ft P
.fi
