[\-\-virtual\-interface\-id <value>]
[\-\-asn <value>]
[\-\-customer\-address <value>]
[\-\-bgp\-peer\-id <value>]
[\-\-cli\-input\-json | \-\-cli\-input\-yaml]
[\-\-generate\-cli\-skeleton <value>]
.ft P
.fi
.UNINDENT
