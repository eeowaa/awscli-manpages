\-\-video <value>
[\-\-client\-request\-token <value>]
[\-\-notification\-channel <value>]
[\-\-face\-attributes <value>]
[\-\-job\-tag <value>]
[\-\-cli\-input\-json | \-\-cli\-input\-yaml]
[\-\-generate\-cli\-skeleton <value>]
.ft P
.fi
.UNINDENT
