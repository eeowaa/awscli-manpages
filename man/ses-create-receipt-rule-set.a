.ft C
  create\-receipt\-rule\-set
\-\-rule\-set\-name <value>
[\-\-cli\-input\-json | \-\-cli\-input\-yaml]
[\-\-generate\-cli\-skeleton <value>]
.ft P
.fi
.UNINDENT
