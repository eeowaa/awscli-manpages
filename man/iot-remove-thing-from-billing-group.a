.INDENT 3.5
.sp
.nf
.ft C
  remove\-thing\-from\-billing\-group
[\-\-billing\-group\-name <value>]
[\-\-billing\-group\-arn <value>]
[\-\-thing\-name <value>]
[\-\-thing\-arn <value>]
[\-\-cli\-input\-json | \-\-cli\-input\-yaml]
[\-\-generate\-cli\-skeleton <value>]
.ft P
.fi
.UNINDENT
.UNINDENT
