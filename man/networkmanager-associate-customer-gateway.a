\-\-customer\-gateway\-arn <value>
\-\-global\-network\-id <value>
\-\-device\-id <value>
[\-\-link\-id <value>]
[\-\-cli\-input\-json | \-\-cli\-input\-yaml]
[\-\-generate\-cli\-skeleton <value>]
.ft P
.fi
.UNINDENT
.UNINDENT
