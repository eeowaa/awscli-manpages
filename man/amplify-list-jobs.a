.INDENT 3.5
.sp
.nf
.ft C
  list\-jobs
\-\-app\-id <value>
\-\-branch\-name <value>
[\-\-cli\-input\-json | \-\-cli\-input\-yaml]
[\-\-starting\-token <value>]
[\-\-page\-size <value>]
[\-\-max\-items <value>]
[\-\-generate\-cli\-skeleton <value>]
.ft P
.fi
