.INDENT 3.5
.sp
.nf
.ft C
{
    "Preset": {
        "Arn": "arn:aws:mediaconvert:us\-east\-1:003235472598:presets/SimpleMP4",
        "Settings": {
        ...<output settings removed for brevity>...
        },
        "Type": "CUSTOM",
        "LastUpdated": 1568938411,
        "Description": "New description text.",
        "Name": "SimpleMP4",
        "CreatedAt": 1568938240
    }
}
.ft P
.fi
