{
    "User": {
        "Username": "7325c1de\-b05b\-4f84\-b321\-9adc6e61f4a2",
        "Enabled": true,
        "UserStatus": "FORCE_CHANGE_PASSWORD",
        "UserCreateDate": 1548099495.428,
        "UserLastModifiedDate": 1548099495.428,
        "Attributes": [
