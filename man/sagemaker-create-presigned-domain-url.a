.ft C
  create\-presigned\-domain\-url
\-\-domain\-id <value>
\-\-user\-profile\-name <value>
[\-\-session\-expiration\-duration\-in\-seconds <value>]
[\-\-expires\-in\-seconds <value>]
[\-\-cli\-input\-json | \-\-cli\-input\-yaml]
[\-\-generate\-cli\-skeleton <value>]
.ft P
.fi
.UNINDENT
.UNINDENT
