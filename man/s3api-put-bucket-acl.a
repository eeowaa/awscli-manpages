.SH SYNOPSIS
.INDENT 0.0
.INDENT 3.5
.sp
.nf
.ft C
  put\-bucket\-acl
[\-\-acl <value>]
[\-\-access\-control\-policy <value>]
\-\-bucket <value>
[\-\-content\-md5 <value>]
[\-\-grant\-full\-control <value>]
[\-\-grant\-read <value>]
[\-\-grant\-read\-acp <value>]
[\-\-grant\-write <value>]
[\-\-grant\-write\-acp <value>]
[\-\-expected\-bucket\-owner <value>]
[\-\-cli\-input\-json | \-\-cli\-input\-yaml]
[\-\-generate\-cli\-skeleton <value>]
.ft P
.fi
.UNINDENT
.UNINDENT
.SH OPTIONS
