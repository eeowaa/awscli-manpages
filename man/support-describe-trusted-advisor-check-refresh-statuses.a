.INDENT 0.0
.INDENT 3.5
.sp
.nf
.ft C
{
    "statuses": [
        {
            "checkId": "Pfx0RwqBli",
            "status": "none",
            "millisUntilNextRefreshable": 0
        },
        {
            "checkId": "zXCkfM1nI3",
            "status": "none",
            "millisUntilNextRefreshable": 0
        }
    ]
}
.ft P
.fi
.UNINDENT
.UNINDENT
.sp
