.ft C
{
  "Filters": [
    {
      "Name": "string",
      "Operator": "Equals"|"NotEquals"|"GreaterThan"|"GreaterThanOrEqualTo"|"LessThan"|"LessThanOrEqualTo"|"Contains"|"Exists"|"NotExists"|"In",
      "Value": "string"
    }
    ...
  ],
  "NestedFilters": [
    {
      "NestedPropertyName": "string",
