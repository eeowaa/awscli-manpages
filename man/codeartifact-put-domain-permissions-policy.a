    "policy": {
        "resourceArn": "arn:aws:codeartifact:region\-id:111122223333:domain/test\-domain",
        "document": "{ ...policy document content...}",
        "revision": "MQlyyTQRASRU3HB58gBtSDHXG7Q3hvxxxxxxx="
    }
}
.ft P
.fi
.UNINDENT
.UNINDENT
.sp
