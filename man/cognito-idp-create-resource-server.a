\-\-user\-pool\-id <value>
\-\-identifier <value>
\-\-name <value>
[\-\-scopes <value>]
[\-\-cli\-input\-json | \-\-cli\-input\-yaml]
[\-\-generate\-cli\-skeleton <value>]
.ft P
.fi
