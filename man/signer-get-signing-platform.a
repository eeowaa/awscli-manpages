{
    "category": "AWS",
    "displayName": "Amazon FreeRTOS SHA1\-RSA CC3220SF\-Format",
    "target": "SHA1\-RSA\-TISHA1",
    "platformId": "AmazonFreeRTOS\-TI\-CC3220SF",
    "signingConfiguration": {
