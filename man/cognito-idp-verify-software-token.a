[\-\-access\-token <value>]
[\-\-session <value>]
\-\-user\-code <value>
[\-\-friendly\-device\-name <value>]
[\-\-cli\-input\-json | \-\-cli\-input\-yaml]
[\-\-generate\-cli\-skeleton <value>]
.ft P
.fi
