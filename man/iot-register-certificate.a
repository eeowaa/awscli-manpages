{
    "certificateArn": "arn:aws:iot:us\-west\-2:123456789012:cert/488b6a7f2acdeb00a77384e63c4e40b18b1b3caaae57b7272ba44c45e3448142",
    "certificateId": "488b6a7f2acdeb00a77384e63c4e40b18b1b3caaae57b7272ba44c45e3448142"
 }
.ft P
.fi
.UNINDENT
.UNINDENT
.sp
