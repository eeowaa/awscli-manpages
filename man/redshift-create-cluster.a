.INDENT 0.0
.INDENT 3.5
.sp
.nf
.ft C
aws redshift create\-cluster \-\-node\-type dw.hs1.xlarge \-\-number\-of\-nodes 2 \-\-master\-username adminuser \-\-master\-user\-password TopSecret1 \-\-cluster\-identifier mycluster
.ft P
.fi
.UNINDENT
.UNINDENT
.sp
