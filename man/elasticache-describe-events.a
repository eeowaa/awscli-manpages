[\-\-source\-identifier <value>]
[\-\-source\-type <value>]
[\-\-start\-time <value>]
[\-\-end\-time <value>]
[\-\-duration <value>]
[\-\-cli\-input\-json | \-\-cli\-input\-yaml]
[\-\-starting\-token <value>]
[\-\-page\-size <value>]
[\-\-max\-items <value>]
[\-\-generate\-cli\-skeleton <value>]
.ft P
.fi
.UNINDENT
.UNINDENT
