[\-\-caller\-id <value>]
[\-\-configuration\-set\-name <value>]
[\-\-content <value>]
[\-\-destination\-phone\-number <value>]
[\-\-origination\-phone\-number <value>]
[\-\-cli\-input\-json | \-\-cli\-input\-yaml]
[\-\-generate\-cli\-skeleton <value>]
.ft P
.fi
