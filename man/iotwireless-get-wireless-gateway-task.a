{
    "WirelessGatewayId": "6c44ab31\-8b4d\-407a\-bed3\-19b6c7cda551",
    "WirelessGatewayTaskDefinitionId": "b7d3baad\-25c7\-35e7\-a4e1\-1683a0d61da9",
    "Status": "Success"
}
